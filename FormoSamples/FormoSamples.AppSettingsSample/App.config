<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <!-- 
   Formo cannot work if keys contains special characters (ie ':','-', ...
   The key should be valid identifier in c#
   -->
  <appSettings>
    <add key="AxServiceUrl" value="http://localhost:1234/SampleService.asmx"/>
    <add key="AxServiceUsername" value="fakeUser"/>
    <add key="AxServicePassword" value="stupidPassword"/>
    <add key="AxServiceTimeout" value="60"/>
  
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
</configuration>